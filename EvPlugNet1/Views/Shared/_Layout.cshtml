<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Plugability</title>
    <link rel="icon" type="image/x-icon" href="/images/favicon1.svg">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EvPlugNet1.styles.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <link href="https://cdn.datatables.net/v/dt/dt-1.13.4/datatables.min.css" rel="stylesheet" />

    <script src="https://cdn.datatables.net/v/dt/dt-1.13.4/datatables.min.js"></script>

    

    @*<script src="~/js/jquery.js"></script>*@

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-HNS43FBPKV"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-HNS43FBPKV');
    </script>

</head>
<body>
    <partial name="_TopNav" />
    
    <div style="clear:both;margin-top:50px;" class="container">
      
        <main role="main" class="pb-3 main" >
            @RenderBody()
        </main>
    </div>

    <partial name="_Footer" />

    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
